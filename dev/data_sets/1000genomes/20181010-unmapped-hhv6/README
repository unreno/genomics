ls -1 *.HHV6a.bowtie2.mapped_center.ratio_unmapped.txt | wc -l
for f in *.HHV6a.bowtie2.mapped_center.ratio_unmapped.txt ; do
c=$( cat $f )
if [ $c != 0 ] ; then
echo $f
fi
done




