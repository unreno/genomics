for vcf in $( ls /raid/data/raw/20170804-Adam/VCF/*.output-hc.snp.vcf ) ; do
  b=$( basename $vcf )
  b=${b%.output-hc.vcf}
  echo $b
  cat $vcf | awk 'BEGIN{FS=OFS="\t"}( !/^#/ ){ print $1,$2,$4,$5 }' > ${b}.snps.txt
  chmod 444 ${b}.snps.txt
done


